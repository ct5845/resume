{
  "name": "vue",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "oh-vue-icons": "^1",
    "pinia": "^2",
    "vue": "^3",
    "vue-router": "^4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1",
    "@tsconfig/node18": "^18",
    "@types/node": "^18",
    "@vitejs/plugin-vue": "^4",
    "@vue/eslint-config-prettier": "^8",
    "@vue/eslint-config-typescript": "^12",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10",
    "daisyui": "^3",
    "eslint": "^8",
    "eslint-plugin-vue": "^9",
    "npm-run-all2": "^6",
    "postcss": "^8",
    "prettier": "^3",
    "tailwindcss": "^3",
    "typescript": "~5.2.0",
    "vite": "^4",
    "vue-tsc": "^1.8.19"
  }
}
